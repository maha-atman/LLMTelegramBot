{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.py",
      "use": "@vercel/python"
    },
    {
      "src": "bot/bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "api/webhook.py"
    },
    {
      "src": "/",
      "dest": "frontend/index.html"
    }
  ]
}
